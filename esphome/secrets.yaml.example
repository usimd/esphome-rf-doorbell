# Secrets file for ESPHome RF Doorbell
# Copy this to secrets.yaml and fill in your actual values

wifi_ssid: "YourWiFiSSID"
wifi_password: "YourWiFiPassword"

ap_password: "FallbackHotspotPassword"

api_key: "your-32-character-api-encryption-key-here"

ota_password: "YourOTAPassword"

# 16-byte encryption key for RFM69 (AES-128)
# Must be exactly 16 characters
rf_encryption_key: "YourRFEncryptKey"
